{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "AssigneePublicId": {
      "type": "string",
      "format": "uuid"
    },
    "TaskPublicId": {
      "type": "string",
      "format": "uuid"
    },
    "Description": {
      "type": "string"
    },
    "ChargePrice": {
      "type": "number"
    },
    "PaymentPrice": {
      "type": "number"
    },
    "CompletedAt": {
      "type": ["string", "null"],
      "format": "date-time"
    }
  },
  "required": ["AssigneePublicId", "TaskPublicId", "Description", "ChargePrice", "PaymentPrice"]
}
